Ninit = 0
Nend 	= 5
n_rep = $(Process) + $INT(Ninit)
Nqueue = $(Nend) - $(Ninit)

executable = /bin/bash
arguments = config/cluster/launch.sh $INT(n_rep) $INT(Nend) EIC_standard 20201219_165454
error = condor_EIC_standard_$INT(n_rep).err
output = condor_EIC_standard_$INT(n_rep).out
log = condor_EIC_standard_$INT(n_rep).log
getenv = true
request_memory = 8000
request_cpus = 1
queue $(Nqueue)